<div class="dash-nav">

    <h5 class="h6">Admin</h5>
</div>
<div class="dash-body ">

    <!--                   Users SECTION-->

    <h3 class="h3">Users</h3>
    <p class="h6">Below You will find a list of valid Users</p>
    <form class="mgV2">
        <div class="searchBox">
            <input placeholder="search" class="search_input" />
            <button class="search_input-icon"><i class="la la-search"></i></button>
        </div>
    </form>
    <div class="mgT4 grid">
        <div class="grid-item-f transaction-table-cont">
           <table class="transaction-table">
               <thead>
               <tr>
<!--                   <th>#</th>-->
<!--                   <th>UserId</th>-->
                   <th>User Name </th>
                   <!--                          ADD IMAGE SPACE-->
                   <th>Current Plan</th>
                   <th>Current Returns</th>
                   <th>Btc Balance</th>
                   <th>Total Invested</th>
                   <th>Images</th>
                   <th></th>
                   <th> </th>
               </tr>
               </thead>
               <tbody>



               {{#each users}}
                   <tr>

                       <td>{{fullName}}</td>

                       <td>{{plan.title}}</td>
                       <td>{{balance.currentReturns}}</td>
                       <td>{{balance.btcBalance}}</td>
                       <td>{{balance.currentInvestment}}</td>
                       <td>
                           <div class="display_flex width35">
                               <img src={{urlProofId}} alt="{{urlProofId}}" class="smlImg"/>
                               <img src="{{urlProofResidence}}" alt="{{urlProofResidence}}" class="smlImg mgL2"/>
                           </div>
                       </td>
                       <td>
                           <a href="/admin/users/edit/{{_id}}" class="btn btn-success">Update</a>

                       </td>
                       <td><a  href="/admin/users/deleteUser/{{_id}}" class="btn btn-red">Delete</a></td>
                   </tr>
               {{/each}}
               </tbody>
           </table>
       </div>
   </div>

</div>